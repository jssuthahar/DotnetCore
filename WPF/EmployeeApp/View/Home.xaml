<Window x:Class="EmployeeApp.View.Home"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EmployeeApp.View"
        mc:Ignorable="d"
        Title="Home" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="File">
                <MenuItem Header="Home" />
                <MenuItem Header="Student" x:Name="Studmenu" Click="Studmenu_Click" />
                <MenuItem Header="Client" x:Name="ClientMenu" Click="ClientMenu_Click" />
                <MenuItem Header="Invoice" x:Name="Invoicemenu" Click="Invoicemenu_Click" />
            </MenuItem>
            <MenuItem Header="Edit">
                <MenuItem Header="Undo" />
                <MenuItem Header="Redo" />
                <MenuItem Header="Cut" />
                <MenuItem Header="Copy" />
                <MenuItem Header="Paste" />
            </MenuItem>
            <MenuItem Header="View">
                <MenuItem Header="Zoom In" />
                <MenuItem Header="Zoom Out" />
                <MenuItem Header="Reset Zoom" />
            </MenuItem>

        </Menu>
        <Button Content="Back" Click="Button_Click">
            
        </Button>
        <Button Content="Fwd" Click="Button_Click_1">

        </Button>
        <Frame NavigationUIVisibility="Hidden" x:Name="MainFrame" ></Frame>
    </DockPanel>
</Window>
