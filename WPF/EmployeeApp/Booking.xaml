<Window x:Class="EmployeeApp.Booking"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EmployeeApp"
        mc:Ignorable="d"
        Title="Booking" Height="450" Width="800">
    <StackPanel>
      
        <TextBlock Text="Name" ></TextBlock>
        <TextBox x:Name="txtName" ></TextBox>
        <TextBlock Text="Email" x:Name="txtslot"></TextBlock>
        <TextBox x:Name="txtemail"  ></TextBox>
        <TextBlock Text="Age"  ></TextBlock>
        <TextBox x:Name="txtage" ></TextBox>
        <TextBlock Text="Password"  ></TextBlock>
        <PasswordBox x:Name="txtpassword"></PasswordBox>
        <Button Content="Register" x:Name="btnreg" Click="btnreg_Click"></Button>
        <Button Content="Login" x:Name="btnlogin" Click="btnlogin_Click"/>

    </StackPanel>
</Window>
